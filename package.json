{
  "name": "nbanewapp",
  "version": "1.0.0",
  "description": "Refactoring old NBA app",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server.js --exec babel-node --presets @babel/preset-env",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild":"NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "MelkeyDev",
  "license": "MIT",
  "dependencies": {
    "antd": "^4.3.4",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "concurrently": "^5.2.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.13",
    "reactstrap": "^8.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "nodemon": "^2.0.3"
  }
}
